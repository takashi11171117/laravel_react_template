{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./resources/ts/*"]
    },
    "types": ["@emotion/react/types/css-prop"]
  },
  "files": [
    "./resources/ts/types/twin.d.ts",
    "./resources/ts/types/vite-env.d.ts",
    "./resources/ts/types/axios.d.ts"
  ],
  "include": ["resources/ts/**/*"],
  "exclude": ["node_modules", "public"]
}
